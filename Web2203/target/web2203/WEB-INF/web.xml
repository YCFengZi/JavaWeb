<!DOCTYPE web-app PUBLIC
        "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd" >
<web-app>
    <display-name>Archetype Created Web Application</display-name>
    <!-- 设置欢迎页 -->
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
    <servlet>
        <servlet-name>atest</servlet-name>
        <servlet-class>net.zjitc.servlet.AServlet</servlet-class>
        <init-param>
            <param-name>name</param-name>
            <param-value>suixinfeng</param-value>
        </init-param>
        <load-on-startup>1</load-on-startup>
    </servlet>

    <servlet>
        <servlet-name>btest</servlet-name>
        <servlet-class>net.zjitc.servlet.BServlet</servlet-class>
        <load-on-startup>2</load-on-startup>
    </servlet>

    <servlet>
        <servlet-name>ctest</servlet-name>
        <servlet-class>net.zjitc.servlet.CServlet</servlet-class>
        <load-on-startup>3</load-on-startup>
    </servlet>

    <servlet>
        <servlet-name>dtest</servlet-name>
        <servlet-class>net.zjitc.servlet.DServlet</servlet-class>
        <load-on-startup>4</load-on-startup>
    </servlet>

    <servlet>
        <servlet-name>mytest</servlet-name>
        <servlet-class>net.zjitc.servlet.MyServlet</servlet-class>
        <load-on-startup>5</load-on-startup>
    </servlet>

    <servlet>
        <servlet-name>myhttptest</servlet-name>
        <servlet-class>net.zjitc.servlet.MyHttpServlet</servlet-class>
        <load-on-startup>6</load-on-startup>
    </servlet>





    <servlet-mapping>
        <servlet-name>atest</servlet-name>
        <url-pattern>/test1</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>btest</servlet-name>
        <url-pattern>/bservlet</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>ctest</servlet-name>
        <url-pattern>/cservlet</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>dtest</servlet-name>
        <url-pattern>/dservlet</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>mytest</servlet-name>
        <url-pattern>/myservlet</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>myhttptest</servlet-name>
        <url-pattern>/myhttpservlet</url-pattern>
    </servlet-mapping>

</web-app>
