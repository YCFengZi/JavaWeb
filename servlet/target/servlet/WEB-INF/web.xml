<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <!-- AServlet -->
  <servlet>
    <servlet-name>aServlet</servlet-name>
    <servlet-class>net.zjitc.servlet.AServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>aServlet</servlet-name>
    <url-pattern>/aservlet</url-pattern>
  </servlet-mapping>
  <!-- BServlet -->
  <servlet>
    <servlet-name>bServlet</servlet-name>
    <servlet-class>net.zjitc.servlet.BServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>bServlet</servlet-name>
    <url-pattern>/bservlet</url-pattern>
  </servlet-mapping>
  <!-- CServlet -->
  <servlet>
    <servlet-name>cServlet</servlet-name>
    <servlet-class>net.zjitc.servlet.CServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>cServlet</servlet-name>
    <url-pattern>/cservlet</url-pattern>
  </servlet-mapping>
  <!-- DServlet -->
  <servlet>
    <servlet-name>dServlet</servlet-name>
    <servlet-class>net.zjitc.servlet.DServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>dServlet</servlet-name>
    <url-pattern>/dservlet</url-pattern>
  </servlet-mapping>
  <!-- MyServlet -->
  <servlet>
    <servlet-name>myServlet</servlet-name>
    <servlet-class>net.zjitc.servlet.MyServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>myServlet</servlet-name>
    <url-pattern>/myservlet</url-pattern>
  </servlet-mapping>
  <!-- MyHttpServlet -->
  <servlet>
    <servlet-name>myHttpServlet</servlet-name>
    <servlet-class>net.zjitc.servlet.MyHttpServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>myHttpServlet</servlet-name>
    <url-pattern>/myhttpservlet</url-pattern>
  </servlet-mapping>
</web-app>
